{
  "name": "unity-asset-price-tracker",
  "version": "1.0.3",
  "description": "This extension lets you view pricing history for Unity assets on site.",
  "private": true,
  "scripts": {
    "build": "rimraf out/* --glob && node ./node_modules/webpack/bin/webpack.js --env production --config config/webpack.config.js && cd out/ && bestzip %npm_package_name%_v%npm_package_version%.zip *",
    "dev": "rimraf out/* --glob && node ./node_modules/webpack/bin/webpack.js --env development --config config/webpack.config.js",
    "watch": "rimraf out/* --glob && node ./node_modules/webpack/bin/webpack.js --env development --watch --config config/webpack.config.js"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.41",
    "@types/chrome": "^0.0.254",
    "@types/jquery": "^3.5.29",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-minimizer-webpack-plugin": "^4.4.0",
    "image-minimizer-webpack-plugin": "^3.8.3",
    "imagemin": "^8.0.1",
    "javascript-obfuscator": "^4.1.0",
    "json-minimizer-webpack-plugin": "^4.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.33",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.3.4",
    "postcss-preset-env": "^9.3.0",
    "sharp": "^0.33.1",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0",
    "webpack-obfuscator": "^3.5.1"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.0.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.2",
    "chart.js": "^4.4.1",
    "jquery": "^3.7.1",
    "rimraf": "^5.0.5"
  },
  "browserslist": [
    ">0%"
  ]
}
